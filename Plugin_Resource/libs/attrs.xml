<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UikitStrokeTextView">
        <attr name="uikitStrokeColor" format="color|reference" />
        <attr name="uikitStrokeWidth" format="float" />
    </declare-styleable>
    <!--自定义item跑马灯控件-->
    <declare-styleable name="UikitCustomizeMarqueeView">
        <attr name="cmvInterval" format="integer|reference" />
        <attr name="cmvAnimDuration" format="integer|reference" />
        <attr name="cmvGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="cmvDirection">
            <enum name="bottom_to_top" value="0" />
            <enum name="top_to_bottom" value="1" />
            <enum name="right_to_left" value="2" />
            <enum name="left_to_right" value="3" />
        </attr>
    </declare-styleable>
</resources>